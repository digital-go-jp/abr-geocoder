  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "geocoding | column",
      "program": "${workspaceFolder}/src/interface/cli/cli.ts",
      "preLaunchTask": "npm: build",
      "runtimeExecutable" : "${workspaceFolder}/.vscode/debug-with-column.sh",
      "runtimeArgs": [
        "--trace-warnings",
        "--require",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/.vscode/test.txt",
        "-f simplified",
        "--silent",
        "--debug"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "geocoding | jq",
      "program": "${workspaceFolder}/src/interface/cli/cli.ts",
      "preLaunchTask": "npm: build",
      "runtimeExecutable" : "${workspaceFolder}/.vscode/debug-with-jq.sh",
      "runtimeArgs": [
        "--trace-warnings",
        "--require",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/.vscode/test.txt",
        "--silent",
        "--debug"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "schools(toCsv)",
      "program": "${workspaceFolder}/src/interface/cli/cli.ts",
      "preLaunchTask": "npm: build",
      "runtimeArgs": [
        "--require",
        "ts-node/register",
        "--inspect",
        "--trace-warnings"
      ],
      "args": [
        "-f",
        "csv",
        "${workspaceFolder}/.vscode/schools-input.txt",
        "${workspaceFolder}/.vscode/schools-output.csv",
        "--debug",
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "short-business-offices(toCsv)",
      "program": "${workspaceFolder}/src/interface/cli/cli.ts",
      "preLaunchTask": "npm: build",
      "runtimeArgs": [
        "--require",
        "ts-node/register",
        "--inspect",
        "--trace-warnings"
      ],
      "args": [
        "-f",
        "csv",
        "${workspaceFolder}/.vscode/short.txt",
        "${workspaceFolder}/.vscode/business-output.csv",
        "--debug",
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "download command",
      "program": "${workspaceFolder}/src/interface/cli/cli.ts",
      "preLaunchTask": "npm: build",
      "runtimeArgs": [
        "--inspect",
        "--require",
        "ts-node/register"
      ],
      "args": [
        "download",
        "-c 082082"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "NODE_ENV": "development"
      }
    },

    {
      "type": "node",
      "request": "launch",
      "name": "api server",
      "program": "${workspaceFolder}/src/interface/cli/cli.ts",
      "preLaunchTask": "npm: build",
      "runtimeArgs": [
        "--trace-warnings",
        "--require",
        "ts-node/register"
      ],
      "args": [
        "serve",
        "--port 80"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "update-check",
      "program": "${workspaceFolder}/src/interface/cli/cli.ts",
      "preLaunchTask": "npm: build",
      "runtimeArgs": [
        "--require",
        "ts-node/register"
      ],
      "args": [
        "update-check",
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    }
  ]
}